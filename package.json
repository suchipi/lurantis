{
  "name": "lurantis",
  "version": "0.2.0",
  "description": "bundler as a service; on-demand npm package bundler over http",
  "main": "index.js",
  "bin": "cli.js",
  "scripts": {
    "clean": "rimraf dist/* && rimraf node_modules",
    "build": "rimraf 'dist/*' && kame bundle --resolver ./kame/resolver.js --input ./src/cli.ts --output ./dist/cli.js",
    "start": "npm run build && ./cli.js"
  },
  "keywords": [
    "bundle",
    "server",
    "npm",
    "http",
    "webpack",
    "babel",
    "service",
    "web",
    "rest"
  ],
  "author": "Lily Scott <me@suchipi.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "arg": "^5.0.1",
    "change-case": "^4.1.2",
    "express": "^4.17.2",
    "kame": "^0.6.4",
    "kleur": "^4.1.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "first-base": "^0.1.3"
  },
  "prettier": {}
}
